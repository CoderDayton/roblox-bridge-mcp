# Roblox Bridge MCP Configuration
# Copy to .env and modify as needed

# Port for the WebSocket bridge server
ROBLOX_BRIDGE_PORT=62847

# Timeout in milliseconds for command execution
ROBLOX_TIMEOUT_MS=30000

# Number of retry attempts for failed commands
ROBLOX_RETRIES=2

# Logging level (DEBUG, INFO, WARN, ERROR)
LOG_LEVEL=INFO
