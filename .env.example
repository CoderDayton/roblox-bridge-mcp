# Roblox Bridge Configuration

# Port for the HTTP bridge server (default: 62847)
ROBLOX_BRIDGE_PORT=62847

# Timeout in milliseconds for command execution (default: 30000)
ROBLOX_TIMEOUT_MS=30000

# Number of retry attempts for failed commands (default: 2)
ROBLOX_RETRIES=2

# Logging level (DEBUG, INFO, WARN, ERROR; default: INFO)
LOG_LEVEL=INFO
